<% 
cc = 0
while cc < @count.to_i
%>
[cache:<%= cc %>]
LINE_RECEIVER_INTERFACE   = <%= @line_receiver_interface %>
LINE_RECEIVER_PORT        = <%= @line_receiver_port.to_i  + (cc.to_i * 10) %>
ENABLE_UDP_LISTENER       = <%= @enable_udp_listener %>
UDP_RECEIVER_INTERFACE    = <%= @udp_receiver_interface %>
UDP_RECEIVER_PORT         = <%= @udp_receiver_port.to_i  + (cc.to_i * 10) %>
PICKLE_RECEIVER_INTERFACE = <%= @pickle_receiver_interface %>
PICKLE_RECEIVER_PORT      = <%= @pickle_receiver_port.to_i  + (cc.to_i * 10) %>
USE_INSECURE_UNPICKLER    = <%= @use_insecure_unpickler %>
CACHE_QUERY_INTERFACE     = <%= @query_interface %>
CACHE_QUERY_PORT          = <%= @query_port.to_i  + (cc.to_i * 10) %>
<% cc += 1 
end %>